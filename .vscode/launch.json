    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'kafka-view'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=kafka-view",
                    "--package=kafka-view"
                ],
                "filter": {
                    "name": "kafka-view",
                    "kind": "bin"
                }
            },
            "args": ["--conf", "config.yaml"],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in executable 'kafka-view'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--bin=kafka-view",
                    "--package=kafka-view"
                ],
                "filter": {
                    "name": "kafka-view",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug example 'consumer_offsets_reader'",
