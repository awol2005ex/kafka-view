                "args": [
                    "build",
                    "--example=consumer_offsets_reader",
                    "--package=kafka-view"
                ],
                "filter": {
                    "name": "consumer_offsets_reader",
                    "kind": "example"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
